{
  "version": 2,
  "builds": [
    {
      "src": "translation_project/requirements.txt",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "builds": [
    { "src": "translation_project/requirements.txt", "use": "@vercel/python" }
  ],
  "env": {
    "PIP_CACHE_DIR": "/vercel/.pip/cache"
  }
}
